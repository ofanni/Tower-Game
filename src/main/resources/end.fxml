<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
      prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="gui.EndController">
    <children>
        <HBox alignment="CENTER" prefHeight="50.0" prefWidth="200.0">
            <children>
                <Label alignment="CENTER" contentDisplay="CENTER" text="Game Over!">
                    <font>
                        <Font name="System Bold" size="28.0"/>
                    </font>
                </Label>
            </children>
        </HBox>
        <VBox alignment="CENTER" prefHeight="70.0" prefWidth="600.0">
            <children>
                <HBox alignment="CENTER" layoutX="10.0" layoutY="27.0">
                    <children>
                        <Label layoutX="10.0" layoutY="10.0" text="Name: "/>
                        <Label fx:id="playerNameLabel"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER">
                    <children>
                        <Label text="Moves: "/>
                        <Label fx:id="moveCountLabel" layoutX="10.0" layoutY="10.0"/>
                    </children>
                </HBox>
                <HBox alignment="CENTER" layoutX="10.0" layoutY="10.0">
                    <children>
                        <Label text="State: "/>
                        <Label fx:id="isCompletedLabel" layoutX="10.0" layoutY="10.0"/>
                    </children>
                </HBox>
            </children>
        </VBox>
        <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0">
            <columns>
            </columns>
        </TableView>
    </children>
</VBox>
